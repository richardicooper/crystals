EDCODE=$COMPCODE
cd $CRYSBUILD
touch $CRYSBUILD/empty
crysedit $CRYSSRC/cryssrc/crystals.ssr crystals.srt code=$EDCODE incl=+ excl=- comm=#  macro=empty strip
crysedit $CRYSSRC/camsrc/cameron.ssr   cameron.srt  code=$EDCODE incl=+ excl=- comm=#  macro=empty strip
crysedit $CRYSSRC/camsrc/button.scm    script/button.cmn   code=$EDCODE incl=+ excl=- comm=?? macro=empty strip
crysedit $CRYSSRC/camsrc/colour.scm    script/colour.cmn   code=$EDCODE incl=+ excl=- comm=?? macro=empty strip
crysedit $CRYSSRC/camsrc/command.scm   script/command.cmn  code=$EDCODE incl=+ excl=- comm=?? macro=empty strip
crysedit $CRYSSRC/camsrc/genhelp.scm   script/genhelp.cmn  code=$EDCODE incl=+ excl=- comm=?? macro=empty strip
crysedit $CRYSSRC/camsrc/order.scm     script/order.cmn    code=$EDCODE incl=+ excl=- comm=?? macro=empty strip
crysedit $CRYSSRC/camsrc/prop.scm      script/prop.cmn     code=$EDCODE incl=+ excl=- comm=?? macro=empty strip
crysedit $CRYSSRC/camsrc/title.scm     script/title.cmn    code=$EDCODE incl=+ excl=- comm=?? macro=empty strip
crysedit $CRYSSRC/bits/rc93/rc93.ssr   rc93.srt     code=$EDCODE incl=+ excl=- comm=#  macro=empty strip
for i in $CRYSSRC/guisrc/*.ssr; 
  do j=${i%.ssr}.srt; crysedit $i ${j##*/} code=$EDCODE incl=+ excl=- comm=#  macro=empty strip;
done;
