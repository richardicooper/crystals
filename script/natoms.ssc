%SCRIPT NATOMS
% VARIABLE INTEGER ATOMSOUT
%% COUNT THE NUMBER OF ATOMS IN L5
%   EVALUATE ATOMSOUT = 0
%   IF EXISTS 5 .NE. 1 THEN
%      FINISH
%   END IF
%   COPY '#GENERALEDIT 5'
%   COPY 'LOCATE RECORDTYPE=101'
%   COPY 'ERROR MESS=NO SIGN=NONE ACTI=CONT NAME=EDITERROR'
%   LOOP
%       ON EDITERROR TERMINATE
%       COPY 'NEXT'
%       EVALUATE ATOMSOUT = LOOPCOUNTER
%   END LOOP
%   COPY 'END'
%END SCRIPT
