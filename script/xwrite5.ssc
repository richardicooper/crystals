%   GET SILENT NOSTORE FINAL ABBREVIATED ' ' ' '                                 
%   IF ( VALUE .EQ. 1 ) THEN                                                     
%    EVALUATE HMODE = 2                                                         
%   END IF                                                                       
%   EVALUATE HREST = FALSE                                                       
%   EVALUATE HRESTF = 2                                                          
^^?? HREST STATE                                                                
%   VERIFY ON OFF                                                                
%   GET SILENT NOSTORE FINAL ABBREVIATED ' ' ' '                                 
%   IF ( VALUE .EQ. 1 ) THEN                                                     
%    EVALUATE HREST = TRUE                                                      
