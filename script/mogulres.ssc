^^WI         @ 2,2 GRID ST NROWS=2 NCOLS=7 {
^^WI           @ 1,1 STATIC RT_T3 'Mean Z:'
^^WI           @ 1,3 STATIC MR_ZAVE '0000.0000'
^^WI           @ 1,5 STATIC RT_T3 'Max Z:'
^^WI           @ 1,7 STATIC MR_ZMAX '0000.0000'
