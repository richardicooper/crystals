^^WI               @ 11,1 STATIC RT_T3 'Lower quartile:'
^^WI               @ 11,3 STATIC MR_LOWERQ '0000.0000'
^^WI               @ 12,1 STATIC RT_T3 'Z-score:'
^^WI               @ 12,3 STATIC MR_ZSCO '0000.0000'
^^WI             }
^^WI           }
^^WI         }
^^WI         @ 6,2 GRID B NROWS=1 NCOLS=6
^^WI         {
^^WI           @ 1,1 STRETCH RT_STR2 HORIZONTAL
^^WI           @ 1,2 BUTTON MR_BOK '&Examine in Mogul'
^^WI           @ 1,4 BUTTON MR_BRR '&Restrain'
^^WI         }
^^WI         @ 8,2 GRID C NROWS=1 NCOLS=1
^^WI         {
^^WI           @ 1,1 BUTTON MR_BOK '&Restrain Everything'
^^WI         }
^^WI       }
^^WI     }
