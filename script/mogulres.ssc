^^WI              'NULL'
^^WI           @ 1,3 GRID GB NROWS=2 NCOLS=1
^^WI           {
^^WI             @ 1,1 STATIC MR_TYPE 'Angle X(000) X(000) X(000)'
^^WI             @ 2,1 GRID GBB NROWS=12 NCOLS=3
^^WI             {
^^WI               @ 1,1 STATIC RT_T3 'Actual:'
^^WI               @ 1,3 STATIC MR_ACTU '0000.0000'
^^WI               @ 3,1 STATIC RT_T3 'Num. hits:'
^^WI               @ 3,3 STATIC MR_NUMO '000000'
^^WI               @ 4,1 STATIC RT_T3 'Median:'
^^WI               @ 4,3 STATIC MR_MEDIAN '0000.0000'
^^WI               @ 5,1 STATIC RT_T3 'Mean:'
^^WI               @ 5,3 STATIC MR_MEAN '0000.0000'
^^WI               @ 6,1 STATIC RT_T3 'Std deviation:'
^^WI               @ 6,3 STATIC MR_SIGM '0000.0000'
