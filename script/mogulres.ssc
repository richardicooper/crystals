%                     IF NUMOBS .GE. 4 THEN
%%
%                       IF Z .GT. 0.0 THEN
%                        EVALUATE ZSUM = ZSUM + Z
%                       ELSE
%                        EVALUATE ZSUM = ZSUM - Z
%                       END IF
%%
%                       EVALUATE ZNUM = ZNUM + 1.0
%%
%                       IF Z .GT. ZMAX THEN
%                         EVALUATE ZMAX = Z
%                       END IF
%                       IF ( - Z ) .GT. ZMAX THEN
