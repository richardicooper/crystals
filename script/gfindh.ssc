% VARIABLE LOGICAL TWINNED                                                      
% EVALUATE TWINNED = FALSE                                                      
%  BLOCK                                                                        
%    VARIABLE INTEGER ITMP                                                      
%    IF ( EXISTS 13 .GT. 0 ) THEN                                               
%        COPY '#GENERALEDIT 13'                                                 
%        COPY 'LOCATE RECORDTYPE=101'                                           
%        COPY 'TRANSFER FROM OFFSET=1 TO ITMP'                                  
%        IF ( ITMP .GE. 0 ) THEN                                                
%         EVALUATE TWINNED = TRUE                                               
