%%
%% Progress window
^^WI WINDOW PROGRESS 'Progress' CANCEL='^^CO DISPOSE PROGRESS'
^^WI GRID  MAIN NROWS=7 NCOLS=3
^^WI {
^^WI  @ 2,2 GRID TOP NROWS=1 NCOLS=3
^^WI  {
^^WI   @ 1,1 STATIC T1 'Experiment is '
^^WI   @ 1,2 STATIC T2 '99.99'
^^WI   @ 1,3 STATIC T3 '% complete.'
^^WI  }
^^WI  @ 4,2 GRID MID NROWS=1 NCOLS=2
^^WI  {
^^WI   @ 1,1 STATIC T4 'Current perturbation = '
^^WI   @ 1,2 STATIC T5 '0.001'
^^WI  }
^^WI  @ 6,2 GRID BOT NROWS=1 NCOLS=2
^^WI  {
^^WI   @ 1,1 STATIC T6 'Time remaining '
^^WI   @ 1,2 STATIC T7 '99.99'
^^WI  }
^^WI }
^^WI SHOW
^^CR
